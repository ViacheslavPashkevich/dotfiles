! Molokai theme
URxvt*background		: #1B1D1E
URxvt*foreground		: #F8F8F2
! URxvt*cursorColor		: #FF0000
! Black         0;30
URxvt*color0			: #1B1D1E
! Red           0;31
URxvt*color1			: #B60050
! Green         0;32
URxvt*color2			: #88BB22
! Brown         0;33
URxvt*color3			: #C47F2C
! Blue          0;34
URxvt*color4			: #30309B
! Purple        0;35
URxvt*color5			: #7E40A5
! Cyan          0;36
URxvt*color6			: #4699AF
! Light Gray    0;37
URxvt*color7			: #9999AA

! Dark Gray     1;30
URxvt*color8			: #303030
! Light Red     1;31
URxvt*color9			: #F92672
! Light Green   1;32
URxvt*color10			: #80FF00
! Yellow        1;33
URxvt*color11			: #FFBA68
! Light Blue    1;34
URxvt*color12			: #7070F0
! Light Purple  1;35
URxvt*color13			: #BB88DD
! Light Cyan    1;36
URxvt*color14			: #66D9EF
! White         1;37
URxvt*color15			: #F8F8F2


! disable yellow rectangles for input unicode symbols (need for Ctrl+Shift+key shortuts)
URxvt.iso14755 : false

! extensions
URxvt*perl-ext: default,tabbedex,clipboard

! clipboard
URxvt.keysym.C-C:   perl:clipboard:copy
URxvt.keysym.C-V:   perl:clipboard:paste
URxvt.keysym.C-A-V: perl:clipboard:paste_escaped
URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard
URxvt.clipboard.autocopy: true

! URxvt.tabbed.tabbar-fg: <colour-index, default 3>
! URxvt.tabbed.tabbar-bg: <colour-index, default 0>
URxvt.tabbed.tab-fg: 15
URxvt.tabbed.tab-bg: 8

URxvt*saveLines: 5000
URxvt*font: xft:dejavu\ sans\ mono:size=9
URxvt*boldFont: xft:dejavu\ sans\ mono:bold:size=9
URxvt*scrollBar: false
URxvt*letterSpace: -1

! urxvt*geometry: 120x40



! yeahconsole.tabbed.tabbar-fg: 2
! yeahconsole.tabbed.tabbar-bg: 0
! yeahconsole.tabbed.tab-fg:    3
! yeahconsole.tabbed.tab-bg:    0

yeahconsole*term: urxvt
! yeahconsole.perl-ext-common: default,tabbed
! yeahconsole*perl-ext: tabbed
yeahconsole*consoleHeight: 30
yeahconsole*xrandrSupport: 1
yeahconsole*yOffset: 25
yeahconsole*aniDelay: 0
yeahconsole*stepSize: 0
yeahconsole*toggleKey: Alt+grave
! yeahconsole*background: #212121
! yeahconsole*foreground: #C0C0C0
! yeahconsole*cursorColor: #E6E3E6
yeahconsole*restart: 1
yeahconsole*keyFull: None+F11
yeahconsole*keySmaller: Control+KP_Subtract
yeahconsole*keyBigger: Control+KP_Add
! yeahconsole*faceName: xft:dejavu\ sans\ mono:size=10
! yeahconsole*boldFont: xft:dejavu\ sans\ mono:bold:size=10
! yeahconsole*metaSendsEscape: true
! yeahconsole*utf8: 2
! yeahconsole*transparent: true
! yeahconsole*shading: 4
! yeahconsole*depth: 32
! yeahconsole*letterSpace: -1



