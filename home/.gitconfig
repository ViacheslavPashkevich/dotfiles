[alias]
        c = commit
	cm = commit -m
	cam = commit -a -m
	st = status
	co = checkout
	last = log -1 HEAD
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[user]
	email = nordsp@gmail.com
	name = Viacheslav Pashkevich
[giggle]
	main-window-maximized = true
	main-window-geometry = 0x0+0+0
	main-window-view = FileView
	history-view-vpane-position = 317
	file-view-vpane-position = 454
[color]
	ui = true
[push]
	default = simple
[core]
	excludesfile = /home/viacheslav/.gitignore_global
